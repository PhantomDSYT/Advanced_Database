@model AD_DB_Project.Models.Airport

@{
    ViewData["Title"] = "Airport Details";
}

<h1 class="text-center">Airport Details</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AirportCode)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AirportCode)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AirportName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AirportName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AirportParish)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AirportParish)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AirportCity)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AirportCity)
        </dd>
    </dl>
</div>
<div class="btn-group">
    <a asp-action="Edit" asp-route-id="@Model.AirportCode" class="btn btn-info">Edit</a> 
    <a asp-action="Index" class="btn btn-primary">Back to List</a>
</div>
